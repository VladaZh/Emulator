# Тестируем обработку ошибок для cp и rmdir
cp nonexistent.txt backup.txt
cp readme.txt /nonexistent_dir/backup.txt
cp readme.txt documents/readme.txt
cp /home/user/documents /backup
cd /home/user
rmdir documents
rmdir nonexistent_dir
rmdir /home/user/documents/readme.txt
rmdir /

# Тестируем обработку ошибок для других команд
cd /nonexistent
ls invalid/path
head /nonexistent_file.txt
head /home
head -n invalid /readme.txt

